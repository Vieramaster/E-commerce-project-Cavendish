{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "strict": true,
    "jsx": "react-jsx",
    "target": "ESNext",
    "lib": ["ESNext", "DOM"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "resolveJsonModule": true
  },
  "include": ["src/**/*"]
}
