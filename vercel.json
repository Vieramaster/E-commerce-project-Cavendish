{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/.*",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/index.html"
    },
    {
      "src": "/shop/:category",
      "dest": "/dist/index.html"
    },
    {
      "src": "/search/:search",
      "dest": "/dist/index.html"
    },
    {
      "src": "/shop/:category/:idProduct",
      "dest": "/dist/index.html"
    },
    {
      "src": "/contact",
      "dest": "/dist/index.html"
    },
    {
      "src": "/delivery",
      "dest": "/dist/index.html"
    },
    {
      "src": "/returns",
      "dest": "/dist/index.html"
    },
    {
      "src": "/product-care",
      "dest": "/dist/index.html"
    },
    {
      "src": "/our-story",
      "dest": "/dist/index.html"
    },
    {
      "src": "/stores",
      "dest": "/dist/index.html"
    },
    {
      "src": "/our-fabrics",
      "dest": "/dist/index.html"
    }
  ]
}