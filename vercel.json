{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/NewsApi.js": {
      "runtime": "nodejs18.x"
    }
  },
  "builds": [
    {
      "src": "api/NewsApi.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ]
}